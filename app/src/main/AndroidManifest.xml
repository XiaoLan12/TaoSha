<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yizhisha.taosha">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.me.settin.SettinActivity" />
        <activity android:name=".ui.me.settin.SetInfoActivity" />
        <activity android:name=".ui.me.settin.changeinfo.ChangeUserNameActivity" />
        <activity android:name=".ui.me.myorder.MyOrderAcitvity" />
        <activity android:name=".ui.me.myorder.OrderDetailsActivity" />
        <activity android:name=".ui.me.mycollect.MyCollectActivity" />
        <activity android:name=".ui.me.myfootprint.MyFootprintActivity" />
        <activity android:name=".ui.me.accountingcenter.AccountCenterActivity" />
        <activity android:name=".ui.me.freesample.FreeSampleActivity" />
        <activity android:name=".ui.me.myrating.MyRatingActivity" />
        <activity android:name=".ui.me.contactus.ContactUsActivity" />
        <activity android:name=".ui.me.abouttaosha.AboutActivity" />
        <activity android:name=".ui.me.abouttaosha.AgreementStatementActivity"></activity>
    </application>

</manifest>